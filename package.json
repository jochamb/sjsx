{
  "name": "@jochamb/sjsx",
  "version": "0.0.6",
  "description": "stateless jsx",
  "author": "jochamb",
  "type": "module",
  "exports": {
    "./jsx-runtime": "./dist/jsx-runtime.js",
    "./package.json": "./package.json",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "precommit": "sort-package-json 'package.json' && git add .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "tsx --test \"src/**/*.test.{ts,tsx}\"",
    "test:only": "tsx --test-only --watch",
    "test:watch": "tsx --test --watch \"src/**/*.test.{ts,tsx}\"",
    "types": "tsc",
    "upgrade-packages": "npm-check-updates -p npm --interactive"
  },
  "lint-staged": {
    "*.{js,css,md,mdx,ts,tsx,json,yml,yaml}": "prettier --write",
    "*.js": "eslint --cache --fix"
  },
  "devDependencies": {
    "@types/node": "22.5.5",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "esbuild": "0.23.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-check-updates": "^16.14.20",
    "prettier": "3.3.2",
    "sort-package-json": "2.10.0",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.16.5",
    "typescript": "5.4.5"
  }
}
